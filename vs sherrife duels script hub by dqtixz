local PabloLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/BatuKvi123/PabloLibV3/main/PabloLibV3"))()
local window = PabloLib:Create(
"Projeto LKA I Murderes V3",
"Enabled",
"p"
)

local tab1 = window:CreateTab("Principal")
local tab2 = window:CreateTab("Mais+")
local tab3 = window:CreateTab("Extras")
local tab4 = window:CreateTab("Creditos")
local tab5 = window:CreateTab("Deletar Menu")

tab1:CreateTextbox("Mudar Sua Volocidade", function(a)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = a
print(a)
end)

tab1:CreateWarning("Recomendando 18-25")
tab1:CreateWarning("é")
tab1:CreateWarning("Recomendando 18-22")

tab1:CreateTextbox("Mudar Seu Pulo", function(a)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = a
print(a)
end)

tab1:CreateWarning("recomendado 70-100")

tab1:CreateButton("Hitbox Pequena", function()
G.HeadSize = 12
_G.Disabled = true
 
game:GetService('RunService').RenderStepped:connect(function()
if _G.Disabled then
for i,v in next, game:GetService('Players'):GetPlayers() do
if v.Name ~= game:GetService('Players').LocalPlayer.Name then
pcall(function()
v.Character.HumanoidRootPart.Size = Vector3.new(_G.HeadSize,_G.HeadSize,_G.HeadSize)
v.Character.HumanoidRootPart.Transparency = 0.5
v.Character.HumanoidRootPart.BrickColor = BrickColor.new("Really black")
v.Character.HumanoidRootPart.Material = "Neon"
v.Character.HumanoidRootPart.CanCollide = false
end)
end
end
end
end)

tab1:CreateWarning("hitbox pequena boa para disfarçar")
tab1:CreateWarning("Não Atire Atrais Da Parede")
tab1:CreateWarning("Atirar Atrais da parede")
tab1:CreateWarning("Pode Ser Visto Como Hacker")
tab1:CreateWarning("Ou Caso Quer Xitar Igual Doido")

